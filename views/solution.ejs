<div class="container-sol">
  
  <% for(var i = 0;i<9;i++) { %>
    <div class="row">
      <% for(var j = 0;j<9;j++ ) { %>
      <input type="number" value=<%=mat[i][j]%>
          class="<%= (j==2 || j==5) ? done[i][j]==0 ? 'break3 empty' : 'break3' : done[i][j]==0 ? 'empty' : 'filled' %>"
        > 
      <% } %> 
    </div>
    <% if (i==2 || i==5) { %>
     <br>
    <% } %>
  <% } %>

</div>